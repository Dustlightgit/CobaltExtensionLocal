{
    "manifest_version": 2,
    "name": "CobaltExtension",
    "version": "1.0",
  
    "description": "Allows you to download videos using cobalt, without having to go to the website!",

    "icons": {
        "48": "icon.png"
    },

    "background": {
        "scripts": ["background.js"],
        "type": "module"
    },

    "permissions": ["activeTab", "contextMenus", "tabs", "downloads"]
  }
  